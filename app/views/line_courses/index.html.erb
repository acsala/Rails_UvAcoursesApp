<h1>Listing line_courses</h1>

<table>
  <thead>
    <tr>
      <th>Course</th>
      <th>Cart</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @line_courses.each do |line_course| %>
      <tr>
        <td><%= line_course.course_id %></td>
        <td><%= line_course.cart_id %></td>
        <td><%= link_to 'Show', line_course %></td>
        <td><%= link_to 'Edit', edit_line_course_path(line_course) %></td>
        <td><%= link_to 'Destroy', line_course, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Line course', new_line_course_path %>
